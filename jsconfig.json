{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "utils/*": ["utils/*"],
      "@pages/*": ["src/pages/*"],
      "@modal/*": ["src/modal/*"],
      "@tests/*": ["tests/*"],
      "@component/*": ["src/component/*"]
    }
  }
}
